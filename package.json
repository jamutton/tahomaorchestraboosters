{
  "name": "tahoma-orchestra-boosters",
  "version": "1.0.0",
  "description": "Tahoma School District Orchestra Boosters website",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:tailwind": "tailwindcss -i ./static/assets/css/input.css -o ./static/assets/css/style.css --watch",
    "build": "npm-run-all build:*",
    "build:eleventy": "eleventy",
    "build:tailwind": "tailwindcss -i ./static/assets/css/input.css -o ./static/assets/css/style.css --minify"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "luxon": "^3.7.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^4.1.18"
  }
}